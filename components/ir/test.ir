@a = global i32 1
@b = global i32 2

back:
    %1 = alloca i32
    %2 = load i32* @a
    store %2, i32* %1
    %3 = load i32* @b
    store %3, i32* @a
    %4 = load i32* %1
    store %4, i32* @b

    %5 = load i32* @a
    %6 = load i32* @b
    blt %5, %6, back, next
next:
    %7 = add %5, %6
    j back
