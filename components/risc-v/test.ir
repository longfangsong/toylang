@a = global i32 1
@b = global i32 2

back:
    %1 = alloca i32
    %2 = load i32* @a
    store %2, i32* %1
    %3 = load i32* @b
    store %3, i32* @a
    %4 = load i32* %1
    store %4, i32* @b

    %5 = load i32* @a
    %6 = load i32* @b
    blt %5, %6, back, next
next:
    %7 = add %5, %6
    %8 = load i32* @a
    %9 = load i32* @a
    %10 = load i32* @a
    %11 = load i32* @a
    %12 = load i32* @a
    %13 = load i32* @a
    %14 = load i32* @a
    %15 = load i32* @a
    %16 = load i32* @a
    %17 = load i32* @a
    %18 = load i32* @a
    %19 = load i32* @a
    %20 = load i32* @a
    %21 = add %8, %9
    %22 = add %10, %11
    %23 = add %12, %13
    %24 = add %14, %15
    %25 = add %16, %17
    %26 = add %18, %19
    %27 = add %20, %21
    %28 = add %21, %20
    %29 = add %20, %19
    %30 = add %29, %19
    %31 = add %28, %19
    %32 = add %27, %19
    %33 = add %26, %19
    j back
