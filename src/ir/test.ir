@a = global u32 1
@b = global u32 2

back:
    %1 = alloca u32
    %2 = load u32* @a
    store %2, u32* %1
    %3 = load u32* @b
    store %3, u32* @a
    %4 = load u32* %1
    store %4, u32* @b

    %5 = load u32* @a
    %6 = load u32* @b
    blt %5, %6, back, next
next:
    %7 = add u32 %5, %6
    j back


    %8 = add u32 %7, %6
    blt %8, %6, lt, nlt
lt:
    %9 = add u32 %6, %4
    j end
nlt:
    %10 = or u32 %6, %4
end:
    %11 = phi u32 [lt, %9], [nlt, %10]