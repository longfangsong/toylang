@a = global i32 1
@b = global i32 2

back:
    %1 = alloca i32
    %2 = load * @a
    store %2, * %1
    %3 = load * @b
    store %3, * @a
    %4 = load * %1
    store %4, * @b

    %5 = load * @a
    %6 = load * @b
    blt %5, %6, back, next
next:
    %7 = add %5, %6
    %8 = load * @a
    %9 = load * @a
    %10 = add %8, %9
    %11 = add %9, %10
    %12 = add %7, %11
    j back
